{
  "manifest_version":2,
  "name": "BEL PRICE CONVERTER",
  "version": "1.0.2",
  "description": "Конвертирует цены из белорусских рублей в у.е. по заданному курсу",
  "background":{
    "scripts":["eventPage.js"],
    "persistent":false
  },
  "content_scripts": [ {

    "js": [ "jquery-2.1.0.min.js","jquery.tooltipster.min.js", "arrive.js", "KupiTutByPlugin.js", "priceConverter_cs.js" ],
    "css": [ "tooltipster.css"],
    "matches": [ "http://*.kupi.tut.by/*", "https://*.kupi.tut.by/*"],
    "run_at":"document_start"
  }],
  "options_page": "options.html",
  "page_action": {
    "default_icon":"coin-us-dollar-icon.png",
    "default_title":"BEL PRICE CONVERTER",
    "default_popup":"popup.html"
  },

  "icons":
  {
     "128" :"coin-us-dollar-icon_128.png"
  },

  "permissions":[
    "tabs",
    "http://*.kupi.tut.by/*",
    "https://*.kupi.tut.by/*",
    "storage"
  ]
}